-- Scripts:

local function VXLGQ_fake_script() -- close.Closebuttonscript 
	local script = Instance.new('LocalScript', close)

	script.Parent.MouseButton1Down:Connect(function()
		script.Parent.Parent.Parent:Destroy()
	end)
end
coroutine.wrap(VXLGQ_fake_script)()
local function XKRXKPJ_fake_script() -- infyieldbutton.infyieldbuttonscript 
	local script = Instance.new('LocalScript', infyieldbutton)

	script.Parent.MouseButton1Down:Connect(function()
		loadstring(game:HttpGet(_G.infydtype))()
	end)
end
coroutine.wrap(XKRXKPJ_fake_script)()
local function RQSVWK_fake_script() -- owlhubbutton.owlhubbbuttonscript 
	local script = Instance.new('LocalScript', owlhubbutton)

	script.Parent.MouseButton1Down:Connect(function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/CriShoux/OwlHub/master/OwlHub.txt"))();
	end)
end
coroutine.wrap(RQSVWK_fake_script)()
